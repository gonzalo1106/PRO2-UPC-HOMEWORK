public class Program
{
    public static void Main(string[] args)
    {
        for (int i = 1; i <= 7; i++)
        {
            Console.WriteLine(FibonacciRecursivo(i));
        }
    }

    public static int FibonacciRecursivo(int n)
    {
        if (n == 1)
        {
            return 1;
        }
        else if (n == 2)
        {
            return 6;
        }
        else
        {
            return FibonacciRecursivo(n - 1) + FibonacciRecursivo(n - 2);
        }
    }
}
